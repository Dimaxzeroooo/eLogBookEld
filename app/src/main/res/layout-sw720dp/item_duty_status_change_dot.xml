<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/whiteee"

    >


    <TextView
        android:id="@+id/dateTimeDiffTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/date_and_time"
        android:paddingLeft="@dimen/view_height_35"
        android:paddingTop="@dimen/margin_13"
        android:paddingBottom="@dimen/margin_13"
        android:textSize="@dimen/text_size_20"
        android:textColor="@color/color_eld_theme"
        android:gravity="left"
        android:maxLines="1"
        android:visibility="gone"

        />


    <LinearLayout
    android:id="@+id/dutyStatusDotLay"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@color/dot_titles_bg"
    android:padding="@dimen/margin_11"

    >

    <TextView
        android:id="@+id/dateTimeDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/date_and_time"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:textColor="@color/color_eld_theme"
        android:gravity="center_horizontal"
        android:layout_weight="1"
        android:maxLines="2"

        />


    <TextView
        android:id="@+id/eventDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Event"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />



        <TextView
            android:id="@+id/geoLocDotTV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/geo_loc"
            android:textStyle="bold"
            android:paddingLeft="@dimen/margin_short_2"
            android:paddingRight="@dimen/margin_short_2"
            android:textSize="@dimen/text_size_18"
            android:gravity="center_horizontal"
            android:textColor="@color/color_eld_theme"
            android:layout_weight="1"
            android:maxLines="2"

            />
        <TextView
            android:id="@+id/latLongDotTV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/lat_long"
            android:textStyle="bold"
            android:paddingLeft="@dimen/margin_short_2"
            android:paddingRight="@dimen/margin_short_2"
            android:textSize="@dimen/text_size_18"
            android:gravity="center_horizontal"
            android:textColor="@color/color_eld_theme"
            android:layout_weight="1"
            android:maxLines="2"

            />
        <TextView
            android:id="@+id/distanceCordDotTV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/dis_last_coord"
            android:textStyle="bold"
            android:paddingLeft="@dimen/margin_short_2"
            android:paddingRight="@dimen/margin_short_2"
            android:textSize="@dimen/text_size_18"
            android:gravity="center_horizontal"
            android:textColor="@color/color_eld_theme"
            android:layout_weight="1"
            android:maxLines="2"

            />


        
        <TextView
        android:id="@+id/cmvDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/cmv"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/distanceAccDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/distance_acc"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/hrsAccDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/hrs_acc"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/distanceTotalDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Distance_total"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/recStatusDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/RecordStatus"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/recOriginDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/RecordOrigin"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/seqNoDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/seq_id"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_18"
        android:gravity="center_horizontal"
        android:textColor="@color/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />


</LinearLayout>

</LinearLayout>