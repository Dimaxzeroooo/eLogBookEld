<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >



    <include
        android:id="@+id/actionBarWiredObd"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/view_height_60"
        layout="@layout/actionbar_eld"
        />




    <ListView
        android:id="@+id/obdDataListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:dividerHeight="5dp"
        android:padding="10dp"
        android:paddingBottom="10dp"
        android:paddingRight="20dp"
        android:paddingLeft="10dp"
        android:layout_marginTop="@dimen/view_height_60"

        />



    <ImageView
        android:id="@+id/clearDataBtn"
        android:layout_width="@dimen/view_height_65"
        android:layout_height="@dimen/view_height_65"
        android:src="@drawable/trash"
        android:textAllCaps="false"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="30dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        />



</RelativeLayout>