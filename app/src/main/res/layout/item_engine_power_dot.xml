<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="?attr/bgColor"

    >


    <TextView
        android:id="@+id/dateTimeDiffEngineTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/date_and_time"
        android:paddingLeft="@dimen/margin_15"
        android:paddingTop="@dimen/margin_short_6"
        android:paddingBottom="@dimen/margin_short_6"
        android:textSize="@dimen/text_size_16"
        android:textColor="?attr/color_eld_theme"
        android:gravity="left"
        android:maxLines="1"
        android:visibility="gone"
        />



    <LinearLayout
        android:id="@+id/enginePwrDotLay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="?attr/dot_list_bg_color"
        android:padding="@dimen/margin_short_6"

    >

    <TextView
        android:id="@+id/dateTimeEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/date_and_time"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:textColor="?attr/color_eld_theme"
        android:gravity="center_horizontal"
        android:layout_weight="1"
        android:maxLines="2"

        />


    <TextView
        android:id="@+id/eventDotETV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Event"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/jeoLocEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/geo_loc"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />


        <TextView
            android:id="@+id/latLonEDotTV"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/lat_long"
            android:textStyle="bold"
            android:paddingLeft="@dimen/margin_short_2"
            android:paddingRight="@dimen/margin_short_2"
            android:textSize="@dimen/text_size_14"
            android:gravity="center_horizontal"
            android:textColor="?attr/color_eld_theme"
            android:layout_weight="1"
            android:maxLines="2"

            />

        <TextView
        android:id="@+id/distanceAccDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/distance_last_coor"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />


    <TextView
        android:id="@+id/cmvEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/cmv"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/distanceTotalEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Distance_total"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />

    <TextView
        android:id="@+id/hoursTotalEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/hours_total"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:paddingRight="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />


    <TextView
        android:id="@+id/seqNoEDotTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/seq_id"
        android:textStyle="bold"
        android:paddingLeft="@dimen/margin_short_2"
        android:textSize="@dimen/text_size_14"
        android:gravity="center_horizontal"
        android:textColor="?attr/color_eld_theme"
        android:layout_weight="1"
        android:maxLines="2"

        />


</LinearLayout>
</LinearLayout>